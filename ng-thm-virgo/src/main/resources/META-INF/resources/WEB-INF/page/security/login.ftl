<body>

    <div class="header">
        <a href="index.html" class="logo centralize"></a>
    </div>    
    
    <div class="login" id="login">
        <div class="wrap">
            <h1>Welcom. Please Log In</h1>
            <form action="index.html" method="post" id="validate">
            <div class="row">
                <div class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                    <input type="text" name="login" placeholder="Login" class="form-control validate[required]"/>
                </div>                                                 
                <div class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-exclamation-sign"></i></span>
                    <input type="password" name="password" placeholder="Password" class="form-control validate[required]"/>
                </div>          
                <div class="dr"><span></span></div>                                
            </div>                
            <div class="row">
                <div class="col-md-8 remember">                    
                    <input type="checkbox" name="rem"/> Remember me                    
                </div>
                <div class="col-md-4    TAR">
                    <input type="submit" class="btn btn-block btn-primary" value="Log In"/>
                </div>
            </div>
            </form>
            <div class="dr"><span></span></div>
            <div class="row">
                <div class="col-md-8">                    
                    <button class="btn btn-block" onClick="loginBlock('#forgot');">Forgot your password?</button>
                </div>
                <div class="col-md-4">
                    <button class="btn btn-warning btn-block" onClick="loginBlock('#sign');">Sign Up</button>
                </div>
            </div>            
        </div>
    </div>
    
    <div class="login" id="sign">
        <div class="wrap">
            <h1>Registration</h1>
            
            <div class="row">
                <div class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                    <input type="text" name="login" placeholder="Login" class="form-control"/>
                </div>
                <div class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                    <input type="text" name="login" placeholder="E-mail" class="form-control"/>
                </div>                
                <div class="dr"><span></span></div>                
                <div class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-exclamation-sign"></i></span>
                    <input type="text" name="password" placeholder="Password" class="form-control"/>
                </div>                          
                <div class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-question-sign"></i></span>
                    <input type="text" name="re-password" placeholder="Re-Password" class="form-control"/>
                </div>                                
                <div class="dr"><span></span></div>                                
            </div>
            <div class="row">
                <div class="col-md-8 remember">
                    <input type="checkbox"/> I agree with terms...
                </div>
                <div class="col-md-4 TAR">
                    <button class="btn btn-block btn-primary">Sign Up</button>
                </div>
            </div>
            <div class="dr"><span></span></div>
            <div class="row">
                <div class="col-md-4">                    
                    <button class="btn btn-block" onClick="loginBlock('#login');">Back</button>
                </div>                
            </div>             
        </div>
    </div>    
    
    <div class="login" id="forgot">
        <div class="wrap">
            <h1>Forgot your password?</h1>
            <div class="row">
                <p>Please enter your email address to recover your password</p>
                <div class="input-group">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                    <input type="text" name="email" placeholder="E-mail" class="form-control"/>
                </div>                                                           
                <div class="dr"><span></span></div>                               
            </div>                   
            <div class="row">
                <div class="col-md-4">                    
                    <button class="btn btn-block" onClick="loginBlock('#login');">Back</button>
                </div>                                
                <div class="col-md-4"></div>
                <div class="col-md-4 TAR">
                    <button class="btn btn-block btn-primary">Recover</button>
                </div>
            </div>                                  
        </div>
    </div>    
    
